{
    "name": "chiron/psr17factoryfinder",
    "type": "library",
    "description": "short description",
    "keywords": [
        "discovery",
        "factory",
        "http",
        "psr-7",
        "psr7",
        "psr-17",
        "psr17"
    ],
    "homepage": "https://github.com/ncou/",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.4",
        "chiron/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Http\\Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Factory\\": "tests"
        }
    },
    "scripts": {
        "test": "bash scripts/test-all-factory-lib.sh",
        "test-factory": "phpunit --testdox --colors=always",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "phpstan": "phpstan analyse -l max --no-progress src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
