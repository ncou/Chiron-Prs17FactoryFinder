{
    "name": "ncou/",
    "type": "library",
    "description": "short description",
    "keywords": [
        "ncou",
        ""
    ],
    "homepage": "https://github.com/ncou/",
    "license": "MIT",
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Http\\Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Factory\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-all-factory": "bash scripts/test-all-factory.sh",
        "test-factory": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
