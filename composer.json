{
    "name": "ncou/",
    "type": "library",
    "description": "short description",
    "keywords": [
        "discovery",
        "factory",
        "http",
        "psr-7",
        "psr7",
        "psr-17",
        "psr17"
    ],
    "homepage": "https://github.com/ncou/",
    "license": "MIT",
    "require": {
        "php" : "~7.1",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "phpstan/phpstan": "^0.10.3",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Http\\Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Factory\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-all-factory": "bash scripts/test-all-factory.sh",
        "test-factory": "phpunit --testdox",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "phpstan": "phpstan analyse -l 7 --no-progress src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
